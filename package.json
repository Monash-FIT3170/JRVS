{
  "name": "server",
  "version": "1.0.0",
  "description": "React web application for teaching kids how to use AI",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all-mac": "npm install && for D in */; do (cd \"$D\" && npm install); done",
    "install-all-win": "npm install && for /D %D in (*) do @if exist \"%D\\package.json\" npm install --prefix \"%D\"",
    "prepare": "husky"
  },
  "author": "Max Verhoef",
  "license": "Apache-2.0",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.8",
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "mongodb": "^6.5.0",
    "mongoose": "^8.3.2",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.1.0",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.0",
    "react-toastify": "^10.0.5",
    "sweetalert2": "^11.12.4",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.4.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "husky": "^9.1.4",
    "prettier": "^3.3.3"
  }
}
